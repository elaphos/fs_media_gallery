					</TCEforms>
				</settings.image.thumb.maxWidth>
				<settings.image.thumb.maxHeight>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_image_thumb_maxHeight</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>1000</max>
							</range>
							<size>4</size>
							<default>100</default>
						</config>
					</TCEforms>
				</settings.image.thumb.maxHeight>
				<settings.enableLightbox>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_detailsInLightbox</label>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
					</TCEforms>
				</settings.enableLightbox>
				<settings.image.maxWidth>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_image_maxWidth</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>1000</max>
