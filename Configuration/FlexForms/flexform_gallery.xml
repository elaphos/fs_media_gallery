						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_image_thumb_maxWidth</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
