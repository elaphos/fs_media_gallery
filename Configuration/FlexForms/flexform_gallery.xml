							<default>500</default>
						</config>
					</TCEforms>
				</settings.image.maxWidth>
				<settings.image.maxHeight>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_image_maxHeight</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>1000</max>
							</range>
							<size>4</size>
							<default>800</default>
						</config>
					</TCEforms>
				</settings.image.maxHeight>
			</el>
		</ROOT>
	</image>
