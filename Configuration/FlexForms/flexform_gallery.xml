							</config>
						</TCEforms>
					</settings.gallery.itemsPerPage>
					<settings.gallery.thumb.maxWidth>
						<TCEforms>
							<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_bemediagallery.xlf:mediagallery_plugin_gallery_thumb_maxWidth</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<range>
									<min>1</min>
									<max>1000</max>
								</range>
								<size>4</size>
								<default>190</default>
							</config>
						</TCEforms>
					</settings.gallery.thumb.maxWidth>
					<settings.gallery.thumb.maxHeight>
