<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
	<sheets>
	<general>
	
    <ROOT>
	  <TCEforms>
       <sheetTitle>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_tab_general</sheetTitle>
      </TCEforms>
        <type>array</type>
        <el>
            <settings.mediagalleries>
                <TCEforms>
                    <label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_mediagalleries</label>
                    <config>
                        <type>select</type>
                        <internal_type>db</internal_type>
                        <size>10</size>
                        <maxitems>1000</maxitems>
						<minitems>0</minitems>
						<!-- itemsProcFunc>MiniFranske\FsMediaGallery\Controller\MediaGalleryController->flexformOptions</itemsProcFunc -->
						<foreign_table>tx_fsmediagallery_domain_model_mediagallery</foreign_table>
                    </config>
                </TCEforms>
            </settings.mediagalleries>
		</el>
	</ROOT>
	</general>
	<gallery>
		<ROOT>
		  <TCEforms>
		   <sheetTitle>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_tab_gallery</sheetTitle>
		  </TCEforms>
			<type>array</type>
			<el>
				<settings.gallery.itemsPerPage>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_gallery_thumb_limit</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>200</max>
							</range>
							<size>4</size>
							<default>8</default>
						</config>
					</TCEforms>
				</settings.gallery.itemsPerPage>
				<settings.gallery.thumb.maxWidth>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_gallery_thumb_maxWidth</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>1000</max>
							</range>
							<size>4</size>
							<default>190</default>
						</config>
					</TCEforms>
				</settings.gallery.thumb.maxWidth>
				<settings.gallery.thumb.maxHeight>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_gallery_thumb_maxHeight</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>1000</max>
							</range>
							<size>4</size>
							<default>135</default>
						</config>
					</TCEforms>
				</settings.gallery.thumb.maxHeight>
			</el>
		</ROOT>
	</gallery>
	<album>
		<ROOT>
		  <TCEforms>
		   <sheetTitle>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_tab_album</sheetTitle>
		  </TCEforms>
			<type>array</type>
			<el>
				<settings.album.itemsPerPage>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_album_thumb_limit</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>1</min>
								<max>200</max>
							</range>
							<size>4</size>
							<default>32</default>
						</config>
					</TCEforms>
				</settings.album.itemsPerPage>
				<settings.album.thumb.maxWidth>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_album_thumb_maxWidth</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>30</min>
								<max>1000</max>
							</range>
							<size>4</size>
							<default>120</default>
						</config>
					</TCEforms>
				</settings.album.thumb.maxWidth>
				<settings.album.thumb.maxHeight>
					<TCEforms>
						<label>LLL:EXT:fs_media_gallery/Resources/Private/Language/locallang_db.xlf:tx_fsmediagallery_plugin_album_thumb_maxHeight</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<range>
								<min>30</min>
								<max>1000</max>
							</range>
							<size>4</size>
							<default>100</default>
						</config>
					</TCEforms>
				</settings.album.thumb.maxHeight>
			</el>
		</ROOT>
